{
  "manifest_version": 3,
  "name": "Duels de l'IA",
  "version": "1.0.0",
  "description": "Extension pour l'atelier p√©dagogique Duel de l'IA",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content-redesign.js"],
      "css": ["css/new-styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "css/new-styles.css",
        "js/content-loader.js",
        "data/content-data.json"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}